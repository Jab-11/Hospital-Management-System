  	<servlet-name>CreateDoctor</servlet-name>
  	<url-pattern>/CreateDoctor</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CreateMedicine</servlet-name>
  	<url-pattern>/CreateMedicine</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CreatePatient</servlet-name>
  	<url-pattern>/CreatePatient</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RetrieveDoctor</servlet-name>
  	<url-pattern>/RetrieveDoctor</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Logout</servlet-name>
  	<url-pattern>/Logout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Discharge</servlet-name>
  	<url-pattern>/Discharge</url-pattern>
  </servlet-mapping>
</web-app>
