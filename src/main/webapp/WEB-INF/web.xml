<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <filter>
  	<filter-name>FilterDoctor</filter-name>
  	<display-name>FilterDoctor</display-name>
  	<description></description>
  	<filter-class>com.filters.FilterDoctor</filter-class>
  </filter>
  <filter>
  	<filter-name>FilterPatient</filter-name>
  	<display-name>FilterPatient</display-name>
  	<description></description>
  	<filter-class>com.filters.FilterPatient</filter-class>
  </filter>
  <filter>
  	<filter-name>FilterMedicine</filter-name>
  	<display-name>FilterMedicine</display-name>
  	<description></description>
  	<filter-class>com.filters.FilterMedicine</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>FilterDoctor</filter-name>
  	<servlet-name>CreateDoctor</servlet-name>
  </filter-mapping>
  <filter-mapping>
  	<filter-name>FilterPatient</filter-name>
	<servlet-name>CreatePatient</servlet-name>
  </filter-mapping>
  <filter-mapping>
