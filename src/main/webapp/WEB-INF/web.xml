  	<servlet-class>com.servlets.CreateDoctor</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CreateMedicine</servlet-name>
  	<display-name>CreateMedicine</display-name>
  	<description></description>
  	<servlet-class>com.servlets.CreateMedicine</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CreatePatient</servlet-name>
  	<display-name>CreatePatient</display-name>
  	<description></description>
  	<servlet-class>com.servlets.CreatePatient</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RetrieveDoctor</servlet-name>
  	<display-name>RetrieveDoctor</display-name>
  	<description></description>
  	<servlet-class>com.servlets.RetrieveDoctor</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Logout</servlet-name>
  	<display-name>Logout</display-name>
  	<description></description>
  	<servlet-class>com.servlets.Logout</servlet-class>
  </servlet>
